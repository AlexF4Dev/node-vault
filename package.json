{
  "name": "node-vault",
  "version": "0.1.1",
  "description": "Javascript client for HashiCorp's Vault",
  "main": "index.coffee",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "debug": "^2.1.3",
    "request": "^2.55.0",
    "mustache": "^2.0.0",
    "coffee-script": "^1.9.2"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "sinon": "^1.14.1",
    "mocha": "^2.2.5",
    "should": "^6.0.1",
    "mocha-lcov-reporter": "0.0.2",
    "coveralls": "^2.11.2"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-watch": "./node_modules/.bin/mocha --require blanket -w",
    "cov-html": "./node_modules/.bin/mocha --require blanket -R html-cov > cov.html",
    "coveralls": "./node_modules/.bin/mocha --require blanket -R mocha-lcov-reporter | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kr1sp1n/node-vault.git"
  },
  "keywords": [
    "vault",
    "hashicorp",
    "secrets",
    "manage",
    "client"
  ],
  "author": "kr1sp1n",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kr1sp1n/node-vault/issues"
  },
  "homepage": "https://github.com/kr1sp1n/node-vault",
  "config": {
    "blanket": {
      "pattern": "index.coffee",
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "spec-cov": {
        "threshold": 100
      }
    }
  }
}

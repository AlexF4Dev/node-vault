{
  "name": "node-vault",
  "version": "0.4.5",
  "description": "Javascript client for HashiCorp's Vault",
  "main": "./lib/node-vault.js",
  "scripts": {
    "test": "_mocha --recursive",
    "cov": "istanbul cover _mocha -- test/",
    "test-watch": "_mocha --watch",
    "coveralls": "istanbul cover _mocha && istanbul-coveralls",
    "docs": "docco src/*.js && node script/generate_feature_doc"
  },
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "bluebird": "3.3.4",
    "debug": "^2.1.3",
    "mustache": "^2.2.1",
    "request-promise": "2.0.1",
    "tv4": "^1.2.7"
  },
  "devDependencies": {
    "docco": "^0.7.0",
    "istanbul": "0.4.3",
    "istanbul-coveralls": "1.0.3",
    "mocha": "2.4.5",
    "should": "8.3.0",
    "sinon": "1.17.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kr1sp1n/node-vault.git"
  },
  "keywords": [
    "vault",
